@model TMC.DATA.TbServicios

@{
    ViewBag.Title = "Administrar Servicios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>

    <title>Tabla Administrador de servicios </title>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" type="text/css" href="~/Estilos/EstilosGeneral.css">
    <link rel="stylesheet" type="text/css" href="~/Estilos/EstilosAdmin.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto|Varela+Round">

</head>
<body id="estiloAdmin">
    <div class="container">
        <div class="table-responsive">
            <div class="table-wrapper">
                <div class="table-title">
                    <div class="row">







                        <div>
                            <div class="collapse" id="navbarToggleExternalContent">

                                <ul class="navbar-nav">
                                    <li>@Html.ActionLink("Administrar Catalogos", "Admin_Catalogos", "TbCatalogos", null, new { @class = "nav-link" })</li>
                                    <li>@Html.ActionLink("Administrar Fotos", "Admin_Fotos", "TbFotos", null, new { @class = "nav-link" })</li>
                                    <li>@Html.ActionLink("Administrar Catalogos Fotos", "Admin_Catalogos_Fotos", "TbCatalogos_Fotos", null, new { @class = "nav-link" })</li>
                                    <li>@Html.ActionLink("Administrar Permisos", "Admin_Permisos", "TbPermisos", null, new { @class = "nav-link" })</li>
                                    <li>@Html.ActionLink("Administrar Roles", "Admin_Roles", "TbRoles", null, new { @class = "nav-link" })</li>
                                    <li>@Html.ActionLink("Administrar Permisos de Roles", "Admin_Roles_Permisos", "TbRoles_TbPermisos", null, new { @class = "nav-link" })</li>
                                    <li>@Html.ActionLink("Administrar Usuarios", "Admin_Users", "Usuario", null, new { @class = "nav-link" })</li>
                                </ul>

                            </div>
                            <nav>

                                <button class="navbar-toggler, btn btn-primary" type="button" data-toggle="collapse" data-target="#navbarToggleExternalContent" aria-controls="navbarToggleExternalContent" aria-expanded="false" aria-label="Toggle navigation">
                                    <span class="material-icons">
                                        arrow_drop_up
                                    </span>
                                    <i style="font-size:15px"> Administrar Servicios</i>
                                </button>
                            </nav>

                        </div>

                        <div class="col-xs-7">
                            <a href="~/Views/TbServicios/Create.cshtml" class="btn btn-primary"><i class="material-icons">&#xE147;</i> <span>Agregar Nuevo Servicio</span></a>
                            <a href="#" class="btn btn-primary"><i class="material-icons">&#xE24D;</i> <span>Exportar a excel</span></a>
                        </div>
                    </div>
                </div>
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Nombre</th>
                            <th>Precio</th>
                            <th>ID Catalogo</th>
                            <th>Estado</th>
                            <th>Accion</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>1</td>
                            @*<td>  @Html.LabelFor(model => model.IDServicio, htmlAttributes: new { @class = "control-label col-md-2" })</td>*@
                            <td>  @Html.LabelFor(model => model.nombre, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>  @Html.LabelFor(model => model.precio, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>  @Html.LabelFor(model => model.IDCatalogo, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>  @Html.LabelFor(model => model.estado, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>
                                <a href="~/Views/TbServicios/Edit.cshtml" class="settings" title="Editar" data-toggle="tooltip"><span class="material-icons">edit</span></a>
                                <a href="~/Views/TbServicios/Delete.cshtml" class="delete" title="Borrar" data-toggle="tooltip"><i class="material-icons">&#xE5C9;</i></a>
                            </td>
                        </tr>
                        <tr>
                            <td>2</td>
                            @*<td>  @Html.LabelFor(model => model.IDServicio, htmlAttributes: new { @class = "control-label col-md-2" })</td>*@
                            <td>  @Html.LabelFor(model => model.nombre, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>  @Html.LabelFor(model => model.precio, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>  @Html.LabelFor(model => model.IDCatalogo, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>  @Html.LabelFor(model => model.estado, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                            <td>
                                <a href="~/Views/TbServicios/Edit.cshtml" class="settings" title="Editar" data-toggle="tooltip"><span class="material-icons">edit</span></a>
                                <a href="~/Views/TbServicios/Delete.cshtml" class="delete" title="Borrar" data-toggle="tooltip"><i class="material-icons">&#xE5C9;</i></a>
                            </td>
                        </tr>

                    </tbody>
                </table>
                <div class="clearfix">

                    <ul class="pagination">
                        <li class="page-item disabled"><a href="#">Anterior</a></li>
                        <li class="page-item active"><a href="#" class="page-link">1</a></li>
                        <li class="page-item"><a href="#" class="page-link">2</a></li>
                        <li class="page-item "><a href="#" class="page-link">3</a></li>
                        <li class="page-item"><a href="#" class="page-link">4</a></li>
                        <li class="page-item"><a href="#" class="page-link">5</a></li>
                        <li class="page-item"><a href="#" class="page-link">Siguiente</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</body>




